<?xml version="1.0" ?>
<config>
    <modules>
        <Webinse_Company>
            <version>0.1.0</version>
        </Webinse_Company>
    </modules>
    <global>
        <models>
            <company>
                <resourceModel>company_resource</resourceModel>
            </company>
            <company_resource>
                <entities>
                    <table_company>
                        <table>webinse_company_entities</table>
                    </table_company>
                </entities>
            </company_resource>
        </models>
        <helpers>
            <company>
                <class>Webinse_Company_Helper</class>
            </company>
        </helpers>
        <resources>
            <company_setup>
                <setup>
                    <module>Webinse_Company</module>
                </setup>
            </company_setup>
        </resources>
    </global>
    <admin>
        <routers>
            <company_admin>
                <use>admin</use>
                <args>
                    <module>Webinse_Company</module>
                    <frontName>company_admin</frontName>
                </args>
            </company_admin>
        </routers>
    </admin>
    <adminhtml>
        <menu>
            <company module="company">
                <title>Company</title>
                <sort_order>77</sort_order>
                <action>company_admin/adminhtml_company</action>
            </company>
        </menu>
    </adminhtml>
</config>
